@model QuestionAnswerVM?

@if (Model != null)
{
    <div id="question_@Model.Question.QuestionId">
        <partial name="_QuestionAnswer" model="Model" />
    </div>
}
else
{
     <div class="alert alert-danger text-center" role="alert">
        <h1>Not Found</h1>
        <p>The question you are looking for does not exist or has been removed.</p>
    </div>
}
@section Scripts {
    <script src="~/js/answer-actions.js"></script>
}